{
  "name": "react-boilerplate-app-scripts",
  "version": "0.0.14",
  "description": "react-boilerplate-app-scripts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dog-days/create-react-boilerplate-app.git"
  },
  "bugs": {
    "url": "https://github.com/dog-days/create-react-boilerplate-app/issues"
  },
  "homepage": "https://github.com/dog-days/create-react-boilerplate-app",
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "bin": {
    "react-boilerplate-app-scripts": "./bin/react-boilerplate-app-scripts.js"
  },
  "scripts": {
    "start": "node ./bin/react-boilerplate-app-scripts.js start",
    "build": "node ./bin/react-boilerplate-app-scripts.js build",
    "view-locale-to-excel": "node ./bin/react-boilerplate-app-scripts.js view-locale-to-excel",
    "vlte": "node ./bin/react-boilerplate-app-scripts.js view-locale-to-excel",
    "excel-to-locale-config": "node ./bin/react-boilerplate-app-scripts.js excel-to-locale-config",
    "etlc": "node ./bin/react-boilerplate-app-scripts.js excel-to-locale-config",
    "create-view": "node ./bin/react-boilerplate-app-scripts.js create-view",
    "cv": "node ./bin/react-boilerplate-app-scripts.js create-view",
    "route": "node ./bin/react-boilerplate-app-scripts.js route",
    "reducer": "node ./bin/react-boilerplate-app-scripts.js reducer",
    "cover": "node ./bin/react-boilerplate-app-scripts.js cover",
    "use": "node ./bin/react-boilerplate-app-scripts.js use"
  },
  "keywords": [
    "react",
    "react-boilerplate-app-scripts"
  ],
  "author": "Samo",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "css-loader": "^0.28.1",
    "detect-port": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.0",
    "html-webpack-plugin": "^2.28.0",
    "node-xlsx": "^0.7.4",
    "object.assign": "^4.0.4",
    "postcss-loader": "^2.0.5",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-boilerplate-app-utils": "^0.0.9",
    "react-dev-utils": "^0.5.2",
    "react-hot-loader": "3.0.0-beta.6",
    "relative": "^3.0.2",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "devDependencies": {
    "object.assign": "^4.0.4",
    "promise": "^7.1.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^4.4.5",
    "react-redux-boilerplate-js": "^0.0.10",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.3",
    "whatwg-fetch": "^2.0.3"
  },
  "peerDependencies": {},
  "react-boilerplate-app-scripts": {
    "language": "en_US",
    "appSrcPath": "template/en_US-src",
    "appPublicPath": "template/en_US-public",
    "historyApiFallback": {
      "verbose": true,
      "rewrites": [
        {
          "from": "/api/(.*)",
          "to": "/mock/$1"
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "react",
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ],
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
