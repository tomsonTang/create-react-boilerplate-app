{
  "name": "react-redux-boilerplate-js",
  "version": "0.0.10",
  "description": "react-redux-boilerplate",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^3.1.3"
  },
  "scripts": {
    "build": "npm run build-es && npm run build-default",
    "build-es": "cross-env BABEL_ENV=es babel ./src -d es",
    "build-default": "cross-env BABEL_ENV=default babel ./src -d libs",
    "build-default-watch": "cross-env BABEL_ENV=default babel ./src -d libs -w",
    "build-es-watch": "cross-env BABEL_ENV=es babel ./src -d es -w"
  },
  "author": "sam",
  "license": "",
  "main": "libs/index.js",
  "jsnext:main": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dog-days/create-react-boilerplate-app.git"
  },
  "bugs": {
    "url": "https://github.com/dog-days/r2-js/issues"
  },
  "homepage": "https://github.com/dog-days/create-react-boilerplate-app",
  "dependencies": {
    "object.assign": "^4.0.4",
    "promise": "^7.1.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.3",
    "whatwg-fetch": "^2.0.3"
  }
}
